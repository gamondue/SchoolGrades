<Window x:Class="SchoolGrades_WPF.frmAnnotationsPopUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SchoolGrades_WPF"
        mc:Ignorable="d"
        Title="Annotazioni in evidenza" Height="350" Width="700" Background="PowderBlue">
    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="7*"></RowDefinition>
        </Grid.RowDefinitions>
        <Label FontSize="20" FontWeight="Bold" Foreground="DarkBlue">Annotazioni in evidenza</Label>
        <DataGrid x:Name="dgwStudentsAllPopUpAnnotations" Grid.Row="1" Background="Gray"></DataGrid>
    </Grid>
</Window>
